{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "authors": [
    "<%= repositoryUrl %>"
  ],
  "moduleType": [
    "amd",
    "es6",
    "node"
  ],
  "keywords": <%= JSON.stringify(appKeywords) %>,
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.2.9",
    "angular-bootstrap": "0.12.0",
    "angular-file-upload": "~2.2.0",
    "angular-loading-bar": "~0.8.0",
    "angular-resource": "~1.2.28",
    "angular-route": "~1.2.28",
    "bootstrap": "3.1.1",
    "html5shiv": "~3.7.3",
    "jquery": "1.9.0",
    "jquery-placeholder": "~2.1.1",
    "respond": "respondJS#~1.4.2",
    "angular-animate": "~1.2.28",
    "es5-shim": "~4.3.1",
    "kindeditor": "*",
    "angular-bootstrap-switch": "~0.4.1",
    "echarts": "~2.2.7",
    "angular-cookies": "~1.2.29"
  },
  "overrides": {
    "es5-shim": {
      "main": [
        "./es5-shim.min.js",
        "./es5-sham.min.js"
      ]
    },
    "kindeditor": {
      "main": [
        "kindeditor.js",
        "lang/zh-CN.js"
      ]
    }
  },
  "resolutions": {
    "angular": "~1.2.9"
  }
}
